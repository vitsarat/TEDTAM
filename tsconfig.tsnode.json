{
  "ts-node": {
    "esm": true,
    "transpileOnly": true,
    "experimentalResolver": true
  },
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ESNext",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["vite/client"]
  }
}